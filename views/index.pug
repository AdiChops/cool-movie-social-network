doctype html
html(lang="en")
    head 
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Document
    body 
        p Hello World
        p(id=`unique${jobTitle}`) #{jobTitle}
        p Here are some numbers 
        ul 
        each number in numbers
            li #{number}
            p #{50 - number}